{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "buildDatabase.js",
    "clearDatabase.js",
    "watch.nodemon.json"
  ],
  "verbose": false,
  "execMap": {
    "js": "node"
  },
  "watch": [
    ".env",
    "./*.js",
    "./*.json"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json"
}
